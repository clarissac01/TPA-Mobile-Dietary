<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Dietary" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/darkgreen</item>
        <item name="colorPrimaryVariant">@color/darkbasilgreen</item>
        <item name="colorOnPrimary">@color/darklighgreen</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/darkbeige</item>
        <item name="colorSecondaryVariant">@color/darkdarkerbeige</item>
        <item name="colorOnSecondary">@color/darkbasilorange</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>